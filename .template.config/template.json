
{
  "$schema": "http://json.schemastore.org/template",
  "author": "ASIM GUNDUZ",
  "classifications": [ "MagicOnion", "MagicOnion Generic", "MagicOnion Generic Template","C#","Template" ],
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "identity": "MagicOnionGenericTemplate",
  "name": "Magic Onion Generic Template",
  "shortName": "magic-onion-generic",
  "sourceName": "Magic.",
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
        "type": "parameter",
        "description": "The target framework for the project.",
        "datatype": "choice",
        "choices": [
            {
                "choice": "net6.0"
            },
            {
                "choice": "net7.0"
            }
        ],
        "defaultValue": "net7.0",
        "replaces": "{TargetFramework}"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [ "src/.vs/**", ".template_config/**" ]
        }
      ]
    }
  ]
}
